<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tumedia.metoo.dao.notice.NoticeDAO">

	<select id="listAllByNotice"
		resultType="com.tumedia.metoo.model.notice.Notice">
		SELECT
		*
		FROM tumedia.NOTICE
	</select>

	<select id="findNoticeDetails"
		resultType="com.tumedia.metoo.model.notice.Notice">
		SELECT
		*
		FROM tumedia.NOTICE
		WHERE ID = #{ID}
	</select>
	
</mapper>